    <section id="search" class="container">
        <div class="sbox">
            <h4>캠핑장 검색</h4>
            <div class="form-row">
                <div class="form-group row col-6">
                    <label class="col-2">지역</label>
                    <select id="area" class="form-control col-10">
                        <option value="전국" selected>전국</option>
                        <option value="서울">서울</option>
                        <option value="경기">경기도</option>
                        <option value="경북">경상북도</option>
                        <option value="경남">경상남도</option>
                        <option value="전북">전라도</option>
                        <option value="전남">전라도</option>
                        <option value="충북">충청북도</option>
                        <option value="충남">충청남도</option>
                        <option value="강원">강원도</option>
                        <option value="제주">제주도</option>
                    </select>
                </div>
                <div class="form-group row col-6">
                    <label for="type" class="col-2">테마</label>
                    <select id="type" class="form-control col-10">
                        <option class="op" value="glamping" selected>글램핑</option>
                        <option class="op" value="auto">오토캠핑</option>
                        <option class="op" value="caravan">카라반</option>
                        <option class="op" value="pension">팬션</option>
                    </select>
                </div>
            </div>
            {{>layout/calender}}
            <a onclick="detailed()" id="search">조회하기</a>
        </div>
    </section>